#pragma once
#include <string>


struct NetworkInfo
{
    NetworkInfo()
    {
        serverIp = "";
        myIp = "";
        joinServerPort = 0;
        receivePort = 0;
        sendPort = 0; 
    }
    ~NetworkInfo() {}
    
    std::string serverIp, myIp;
    int joinServerPort, receivePort, sendPort; 
};

